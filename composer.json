{
    "name": "schmidtwebmedia/grid-for-container",
    "type": "typo3-cms-extension",
    "description": "Add an easy to use grid element for Container. It's Framework independent. Prepared for Bootstrap.",
    "keywords": [
        "TYPO3",
        "extension",
        "bootstrap",
        "container",
        "grid"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Marco Schmidt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "typo3/cms-core": "^10.4 || ^11.5 || ^12.4",
        "b13/container": "^1 || ^2",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "SchmidtWebmedia\\GridForContainer\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/grid_for_container": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "grid_for_container"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
